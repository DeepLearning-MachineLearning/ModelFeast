{
    "arch": {
        "args": {
            "img_size": [
                60,
                60
            ],
            "n_class": 17,
            "pretrained": false,
            "pretrained_path": "./pretrained/"
        },
        "type": "xception"
    },
    "metrics": [
        "accuracy"
    ],
    "n_gpu": 1,
    "name": "xception",
    "optimizer": {
        "args": {
            "weight_decay": 3e-05,
            "lr": 0.0003
        },
        "type": "Adam"
    },
    "lr_scheduler": {
        "args": {
            "step_size": 50,
            "gamma": 0.2
        },
        "type": "StepLR"
    },
    "trainer": {
        "verbose_per_epoch": 100,
        "verbosity": 2,
        "tensorboardX": false,
        "epochs": 1,
        "monitor": "max val_accuracy",
        "log_dir": "saved/runs",
        "save_period": 1,
        "early_stop": 10,
        "steps_update": 1,
        "save_dir": "saved/"
    },
    "loss": "cls_loss",
    "data_loader": {
        "args": {
            "shuffle": true,
            "data_dir": "E:/Oxford_Flowers17/train",
            "validation_split": 0.2,
            "transform": [
                60,
                60
            ],
            "batch_size": 64
        },
        "type": "AutoDataLoader"
    }
}