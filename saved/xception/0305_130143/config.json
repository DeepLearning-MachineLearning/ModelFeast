{
    "lr_scheduler": {
        "args": {
            "gamma": 0.2,
            "step_size": 50
        },
        "type": "StepLR"
    },
    "optimizer": {
        "args": {
            "weight_decay": 3e-05,
            "lr": 0.0003
        },
        "type": "Adam"
    },
    "data_loader": {
        "args": {
            "batch_size": 64,
            "validation_split": 0.2,
            "transform": [
                60,
                60
            ],
            "data_dir": "/home/TensorFlow/keras/flowers17-tsycnh package/flowers17/train",
            "shuffle": true
        },
        "type": "AutoDataLoader"
    },
    "metrics": [
        "accuracy"
    ],
    "trainer": {
        "steps_update": 1,
        "verbosity": 2,
        "log_dir": "saved/runs",
        "verbose_per_epoch": 100,
        "epochs": 3,
        "monitor": "max val_accuracy",
        "save_period": 1,
        "early_stop": 10,
        "tensorboardX": false,
        "save_dir": "saved/"
    },
    "n_gpu": 1,
    "arch": {
        "args": {
            "pretrained_path": "./pretrained/",
            "img_size": [
                60,
                60
            ],
            "pretrained": false,
            "n_class": 17
        },
        "type": "xception"
    },
    "name": "xception",
    "loss": "cls_loss"
